{
    "manifest_version": 2,
    "name": "Dofus Hunt Fast Travel",
    "version": "1.0",
    "description": "Provide a button on Dafus Hunt Webside to copy the position with /travel command.",
    "content_scripts": [
        {
            "matches": [
                "https://dofus-map.com/*/hunt"
            ],
            "js": ["fasttravel.js"]
        }
    ],

    "page_action": {
        "default_icon": {                
            "16": "images/icon16x16.png",        
            "32": "images/icon32x32.png",         
            "96": "images/icon96x96.png"          
        },
        "default_title" : "Dofus Hunt Fast Travel"
    }
}

